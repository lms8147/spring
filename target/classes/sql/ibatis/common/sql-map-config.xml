<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
"http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>

	<settings enhancementEnabled="true" maxTransactions="5" maxRequests="32" maxSessions="10"
		useStatementNamespaces="true" />


	<typeHandler javaType="com.mysample.common.firewallmgmt.vo.DeleteYN"
		callback="com.mysample.common.firewallmgmt.vo.DeleteYN$DeleteYNEnumTypeHandler" />
	<typeHandler javaType="com.mysample.common.firewallmgmt.vo.FirewallBound"
		callback="com.mysample.common.firewallmgmt.vo.FirewallBound$FirewallBoundEnumTypeHandler" />
	<typeHandler javaType="com.mysample.common.firewallmgmt.vo.SystemPort"
		callback="com.mysample.common.firewallmgmt.vo.SystemPort$SystemPortEnumTypeHandler" />

	<sqlMap resource="sql/ibatis/common/base/base-mapping.xml" />
	<sqlMap resource="sql/ibatis/common/firewallmgmt/firewallmgmt-sql-mapping.xml" />

</sqlMapConfig>





